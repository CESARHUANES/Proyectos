<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.cardif.satelite.configuracion.dao.UsoVehiculoMapper">
	
	<resultMap id="BaseResultMap" type="com.cardif.satelite.model.UsoVehiculo">
		<id column="COD_USO" property="codUso" jdbcType="VARCHAR" />
		<result column="DESCRIPCION_USO" property="descripcionUso" jdbcType="VARCHAR" />
	</resultMap>
	
	<sql id="Base_Column_List">
		COD_USO, DESCRIPCION_USO
	</sql>
	
	<select id="selectByPrimaryKey" resultMap="BaseResultMap" parameterType="java.lang.String">
		select
		<include refid="Base_Column_List" />
		from USO_VEHICULO
		where COD_USO = #{codUso,jdbcType=VARCHAR}
	</select>
</mapper>