<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.cardif.satelite.configuracion.dao.MarcaVehiculoMapper">
	
	<resultMap id="BaseResultMap" type="com.cardif.satelite.model.MarcaVehiculo">
		<id column="ID" property="id" jdbcType="NUMERIC" />
		<result column="NOMBRE_MARCAVEHICULO" property="nombreMarcavehiculo" jdbcType="VARCHAR" />
	</resultMap>
	
	<sql id="Base_Column_List">
		ID, NOMBRE_MARCAVEHICULO
	</sql>
	
	<select id="selectByPrimaryKey" resultMap="BaseResultMap" parameterType="java.lang.Long">
		select
		<include refid="Base_Column_List" />
		from MARCA_VEHICULO
		where ID = #{id,jdbcType=NUMERIC}
	</select>
	
	<insert id="insertSelective" parameterType="com.cardif.satelite.model.MarcaVehiculo">
		<selectKey keyProperty="id" order="BEFORE" resultType="java.lang.Long">
			SELECT MARCA_VEHICULO_SEQ.NEXTVAL FROM DUAL
		</selectKey>
		
		insert into MARCA_VEHICULO
		<trim prefix="(" suffix=")" suffixOverrides=",">
			<if test="null != id">
				ID,
			</if>
			<if test="null != nombreMarcavehiculo">
				NOMBRE_MARCAVEHICULO,
			</if>
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides=",">
			<if test="id != null">
				#{id, jdbcType=NUMERIC},
			</if>
			<if test="nombreMarcavehiculo != null">
				#{nombreMarcavehiculo, jdbcType=VARCHAR},
			</if>
		</trim>
	</insert>
</mapper>