<?xml version="1.0" encoding="UTF-8"?>
<SSC>
	<ErrorContext />
	<User />
	<SunSystemsContext>
		<BusinessUnit>E01</BusinessUnit>
		<BudgetCode>A</BudgetCode>
	</SunSystemsContext>
	<Payload>
		<Filter>
			<Expr operator="AND">
				<Item name="/Ledger/Line/AccountCode" operator="BETWEEN" minvalue="P" maxvalue="PZZZZZZZZZZZZZ" />
				<Item name="/Ledger/Line/AccountingPeriod" operator="BETWEEN" minvalue="0052013" maxvalue="0022014" />
				<Item name="/Ledger/Line/DebitCredit" operator="GTE" value="B" />
				<Item name="/Ledger/Line/AllocationMarker" operator="EQU" value="5" />
			</Expr>
		</Filter>
		<Select>
			<Ledger>
				<Line>
					<AccountCode />
					<TransactionDate />
					<TransactionReference />
					<CurrencyCode />
					<BaseAmount />
					<TransactionAmount />
					<Base2ReportingAmount />
					<Description />
					<AccountingPeriod />
					<AnalysisCode1 />
					<AnalysisCode2 />
					<AnalysisCode3 />
					<AnalysisCode4 />
					<AnalysisCode5 />
					<AnalysisCode6 />
					<AnalysisCode7 />
					<AnalysisCode8 />
					<AnalysisCode9 />
					<AnalysisCode10 />
					<JournalNumber />
					<BaseRate />
					<Base2ReportingRate />
					<Accounts>
						<Description />
						<LookupCode />
					</Accounts>
				</Line>
			</Ledger>
		</Select>
	</Payload>
</SSC>