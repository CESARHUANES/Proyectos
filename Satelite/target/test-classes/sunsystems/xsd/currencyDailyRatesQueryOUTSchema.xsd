<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema jaxb:version="1.0" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jaxb="http://java.sun.com/xml/ns/jaxb">
	<xsd:element name="SSC">
		<xsd:annotation>
			<xsd:documentation>SSC</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="ErrorContext" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="CompatibilityMode" type="SunSystemsCompatibilityMode" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Compatibilidad de error</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="ErrorOutput" type="SunSystemsErrorOutput" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Detalles de errores.</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="ErrorThreshold" type="SunSystemsErrorThreshold" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Umbral de errores</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="User">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="Name" type="SunSystemsUserName">
								<xsd:annotation>
									<xsd:documentation>Nombre de usuario</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="SunSystemsContext">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="BusinessUnit" type="SunSystemsBusinessUnit">
								<xsd:annotation>
									<xsd:documentation>Base de datos</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="Payload" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Payload</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="DailyConversionRate" minOccurs="0" maxOccurs="unbounded">
								<xsd:annotation>
									<xsd:documentation>Tasas de conversión diarias</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="AccountFrom" minOccurs="0" type="typeCuentaDe">
											<xsd:annotation>
												<xsd:documentation>Cuenta: De</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="AccountRangeTo" minOccurs="0" type="typeCuentaA">
											<xsd:annotation>
												<xsd:documentation>Cuenta: A</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="ConversionRate" minOccurs="0" type="typeTasaConversión">
											<xsd:annotation>
												<xsd:documentation>Tasa conversión</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="ConversionRateDecimalPlaces" minOccurs="0" type="typePosDecimalesTasaConversión">
											<xsd:annotation>
												<xsd:documentation>Pos. decimales tasa conversión</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="CurrencyCodeFrom" minOccurs="0" type="typeCódigoMonedaDe">
											<xsd:annotation>
												<xsd:documentation>Código moneda: De</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="CurrencyCodeTo" minOccurs="0" type="typeCódigoMonedaA">
											<xsd:annotation>
												<xsd:documentation>Código moneda: A</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="DateTimeLastUpdated" minOccurs="0" type="SunSystemsDateTime">
											<xsd:annotation>
												<xsd:documentation>Fecha/hora últ.actualización</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="Description" minOccurs="0" type="typeDescripción">
											<xsd:annotation>
												<xsd:documentation>Descripción</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="EffectiveDate" minOccurs="0" type="SunSystemsDate">
											<xsd:annotation>
												<xsd:documentation>Fecha efectiva: De</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="EffectiveDateTo" minOccurs="0" type="SunSystemsDate">
											<xsd:annotation>
												<xsd:documentation>Fecha efectiva: A</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="LookupCode" minOccurs="0" type="typeCódigoBúsqueda">
											<xsd:annotation>
												<xsd:documentation>Código búsqueda</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="MultiplyOrDivide" minOccurs="0" type="typeMultiplicarODividir">
											<xsd:annotation>
												<xsd:documentation>Multiplicar o dividir</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="RateTolerance" minOccurs="0" type="typeToleranciaDeTasa">
											<xsd:annotation>
												<xsd:documentation>Tolerancia de tasa</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="RateTolerancePercentage" minOccurs="0" type="typePorcentajeToleranciaDeTasa">
											<xsd:annotation>
												<xsd:documentation>Porcentaje tolerancia de tasa</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="RealizedGainsAccount" minOccurs="0" type="typeCuentaGananciasRealizadas">
											<xsd:annotation>
												<xsd:documentation>Cuenta ganancias realizadas</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="RealizedNetOrLossesAccount" minOccurs="0" type="typeCtaNetopérdidasRealizadas">
											<xsd:annotation>
												<xsd:documentation>Cta. neto/pérdidas realizadas</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="ShortHeading" minOccurs="0" type="typeEncabezadoCorto">
											<xsd:annotation>
												<xsd:documentation>Encabezado corto</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="Status" minOccurs="0" type="typeEstado">
											<xsd:annotation>
												<xsd:documentation>Estado</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="UpdateCount" minOccurs="0" type="typeRecuentoActualizaciones">
											<xsd:annotation>
												<xsd:documentation>Recuento actualizaciones</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="UserIdLastUpdated" minOccurs="0" type="typeIdUsuarioÚltactualización">
											<xsd:annotation>
												<xsd:documentation>Id usuario últ.actualización</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="FinancialPeriod" minOccurs="0" maxOccurs="unbounded">
											<xsd:annotation>
												<xsd:documentation>Período financiero</xsd:documentation>
											</xsd:annotation>
											<xsd:complexType>
												<xsd:sequence>
													<xsd:element name="PeriodID" minOccurs="0" type="SunSystemsString" />
												</xsd:sequence>
												<xsd:attributeGroup ref="OutputLog" />
											</xsd:complexType>
										</xsd:element>
										<xsd:element name="Rates" minOccurs="0" maxOccurs="unbounded">
											<xsd:annotation>
												<xsd:documentation>Tasas</xsd:documentation>
											</xsd:annotation>
											<xsd:complexType>
												<xsd:sequence>
													<xsd:element name="RateType" minOccurs="0" type="SunSystemsString" />
												</xsd:sequence>
												<xsd:attributeGroup ref="OutputLog" />
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
									<xsd:attributeGroup ref="OutputLog" />
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="ErrorMessages" type="ErrorMessages" minOccurs="0" />
			</xsd:sequence>
			<xsd:attribute name="SequenceID" type="xsd:string" use="optional" />
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="ErrorMessages">
		<xsd:sequence>
			<xsd:element name="Message" minOccurs="0" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="PayloadRef" type="xsd:unsignedInt" minOccurs="0" />
						<xsd:element name="UserText" type="xsd:string" minOccurs="0" />
						<xsd:element name="Exception" type="xsd:string" minOccurs="0" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="Status">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="success" />
			<xsd:enumeration value="fail" />
			<xsd:enumeration value="partial" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:attributeGroup name="OutputLog">
		<xsd:attribute name="status" type="Status">
			<xsd:annotation>
				<xsd:appinfo>
					<jaxb:property name="internalSSCTypestatus" />
				</xsd:appinfo>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="rejected" type="xsd:boolean">
			<xsd:annotation>
				<xsd:appinfo>
					<jaxb:property name="internalSSCTyperejected" />
				</xsd:appinfo>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="Reference" type="xsd:unsignedInt">
			<xsd:annotation>
				<xsd:appinfo>
					<jaxb:property name="internalSSCTypeReference" />
				</xsd:appinfo>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:attributeGroup>
	<xsd:simpleType name="Level">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="warning" />
			<xsd:enumeration value="error" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:attributeGroup name="MessageAttributes">
		<xsd:attribute name="Level" type="Level">
			<xsd:annotation>
				<xsd:appinfo>
					<jaxb:property name="internalSSCTypeLevel" />
				</xsd:appinfo>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="Reference" type="xsd:unsignedInt">
			<xsd:annotation>
				<xsd:appinfo>
					<jaxb:property name="internalSSCTypeReference" />
				</xsd:appinfo>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:attributeGroup>
	<xsd:simpleType name="SunSystemsUserName">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[A-Z0-9]{1,3}" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SunSystemsCompatibilityMode">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="0">
				<xsd:annotation>
					<xsd:documentation>Off</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="1">
				<xsd:annotation>
					<xsd:documentation>On</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SunSystemsErrorOutput">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="0">
				<xsd:annotation>
					<xsd:documentation>Limited</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="1">
				<xsd:annotation>
					<xsd:documentation>Normal</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="2">
				<xsd:annotation>
					<xsd:documentation>Extended</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="3">
				<xsd:annotation>
					<xsd:documentation>Debug</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SunSystemsErrorThreshold">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="0">
				<xsd:annotation>
					<xsd:documentation>Warning</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="1">
				<xsd:annotation>
					<xsd:documentation>Error</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SunSystemsBusinessUnit">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[A-Z][A-Z0-9]{0,2}" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SunSystemsDataSource">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="32" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SunSystemsBudgetCode">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[A-K]{1}" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="SunSystemsString">
		<xsd:simpleContent>
			<xsd:extension base="xsd:string">
				<xsd:attributeGroup ref="OutputLog" />
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="SunSystemsDecimal">
		<xsd:simpleContent>
			<xsd:extension base="xsd:decimal">
				<xsd:attributeGroup ref="OutputLog" />
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="SunSystemsDate">
		<xsd:simpleContent>
			<xsd:restriction base="SunSystemsString">
				<xsd:pattern value="(([0-2][0-9]|3[01])(0[1-9]|1[0-2])[0-9][0-9][0-9][0-9])?" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="SunSystemsDateTime">
		<xsd:simpleContent>
			<xsd:restriction base="SunSystemsString">
				<xsd:pattern value="(([0-2][0-9]|3[01])(0[1-9]|1[0-2])[0-9][0-9][0-9][0-9]([01][0-9]|2[0-3])[0-5][0-9])?" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="typeCtaNetopérdidasRealizadas">
		<xsd:simpleContent>
			<xsd:restriction base="SunSystemsString">
				<xsd:maxLength value="15" />
				<xsd:pattern value="[^a-z]*" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="typeCuentaA">
		<xsd:simpleContent>
			<xsd:restriction base="SunSystemsString">
				<xsd:maxLength value="15" />
				<xsd:pattern value="[^a-z]*" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="typeCuentaDe">
		<xsd:simpleContent>
			<xsd:restriction base="SunSystemsString">
				<xsd:maxLength value="15" />
				<xsd:pattern value="[^a-z]*" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="typeCuentaGananciasRealizadas">
		<xsd:simpleContent>
			<xsd:restriction base="SunSystemsString">
				<xsd:maxLength value="15" />
				<xsd:pattern value="[^a-z]*" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="typeCódigoBúsqueda">
		<xsd:simpleContent>
			<xsd:restriction base="SunSystemsString">
				<xsd:maxLength value="15" />
				<xsd:pattern value="[^a-z]*" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="typeCódigoMonedaA">
		<xsd:simpleContent>
			<xsd:restriction base="SunSystemsString">
				<xsd:maxLength value="5" />
				<xsd:pattern value="[^a-z]*" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="typeCódigoMonedaDe">
		<xsd:simpleContent>
			<xsd:restriction base="SunSystemsString">
				<xsd:maxLength value="5" />
				<xsd:pattern value="[^a-z]*" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="typeDescripción">
		<xsd:simpleContent>
			<xsd:restriction base="SunSystemsString">
				<xsd:maxLength value="50" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="typeEncabezadoCorto">
		<xsd:simpleContent>
			<xsd:restriction base="SunSystemsString">
				<xsd:maxLength value="15" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="typeEstado">
		<xsd:simpleContent>
			<xsd:restriction base="SunSystemsString">
				<xsd:enumeration value="0">
					<xsd:annotation>
						<xsd:documentation>Abierto</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="1">
					<xsd:annotation>
						<xsd:documentation>Oculto</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="2">
					<xsd:annotation>
						<xsd:documentation>Suspendido/retenido</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="3">
					<xsd:annotation>
						<xsd:documentation>Cerrado/completo</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="typeIdUsuarioÚltactualización">
		<xsd:simpleContent>
			<xsd:restriction base="SunSystemsString">
				<xsd:maxLength value="3" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="typeMultiplicarODividir">
		<xsd:simpleContent>
			<xsd:restriction base="SunSystemsString">
				<xsd:enumeration value="0">
					<xsd:annotation>
						<xsd:documentation>*</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="1">
					<xsd:annotation>
						<xsd:documentation>/</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="typePorcentajeToleranciaDeTasa">
		<xsd:simpleContent>
			<xsd:restriction base="SunSystemsDecimal">
				<xsd:totalDigits value="10" />
				<xsd:fractionDigits value="5" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="typePosDecimalesTasaConversión">
		<xsd:simpleContent>
			<xsd:restriction base="SunSystemsString">
				<xsd:enumeration value="0">
					<xsd:annotation>
						<xsd:documentation>0</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="1">
					<xsd:annotation>
						<xsd:documentation>1</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="2">
					<xsd:annotation>
						<xsd:documentation>2</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="3">
					<xsd:annotation>
						<xsd:documentation>3</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="4">
					<xsd:annotation>
						<xsd:documentation>4</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="5">
					<xsd:annotation>
						<xsd:documentation>5</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="6">
					<xsd:annotation>
						<xsd:documentation>6</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="7">
					<xsd:annotation>
						<xsd:documentation>7</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="8">
					<xsd:annotation>
						<xsd:documentation>8</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="9">
					<xsd:annotation>
						<xsd:documentation>9</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="typeRecuentoActualizaciones">
		<xsd:simpleContent>
			<xsd:restriction base="SunSystemsDecimal">
				<xsd:totalDigits value="4" />
				<xsd:fractionDigits value="0" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="typeTasaConversión">
		<xsd:simpleContent>
			<xsd:restriction base="SunSystemsDecimal">
				<xsd:totalDigits value="20" />
				<xsd:fractionDigits value="9" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="typeToleranciaDeTasa">
		<xsd:simpleContent>
			<xsd:restriction base="SunSystemsString">
				<xsd:enumeration value="0">
					<xsd:annotation>
						<xsd:documentation>No</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="1">
					<xsd:annotation>
						<xsd:documentation>Sí</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
</xsd:schema>
