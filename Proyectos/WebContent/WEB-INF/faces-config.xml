<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
    <navigation-rule>
		<from-view-id>paginas/clientes/listadoClientes.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>actualizaCliente</from-outcome>
			<to-view-id>paginas/clientes/mantenimientoClientes.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	 <navigation-rule>
		<from-view-id>paginas/clientes/mantenimientoClientes.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>listaCliente</from-outcome>
			<to-view-id>paginas/clientes/listadoClientes.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>paginas/usuarios/listadoUsuarios.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>updateUsuario</from-outcome>
			<to-view-id>paginas/usuarios/mantenimientoUsuarios.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	 <navigation-rule>
		<from-view-id>paginas/usuarios/mantenimientoUsuarios.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>listaUsuario</from-outcome>
			<to-view-id>paginas/usuarios/listadoUsuarios.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
<application>
        <message-bundle>com.proyecto.recursos.mensajes</message-bundle>
        <locale-config>
            <default-locale>es</default-locale>
            
        </locale-config>        
        <resource-bundle>
            <base-name>com.proyecto.recursos.mensajes</base-name>
            <var>msjs</var>
        </resource-bundle>
    </application>
</faces-config>
